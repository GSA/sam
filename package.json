{
  "name": "@gsa-sam/sam-styles",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "fractal:build": "fractal build",
    "federalist": "npm install --only=dev && npm run postinstall && npm run prod && npm run fractal:build",
    "start": "fractal start --sync",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "webpack --env.mode development --watch --progress",
    "prod": "webpack --env.mode production",
    "clean": "rimraf dist",
    "copy:fonts": "copy './node_modules/uswds/dist/fonts/**/*.*' ./src/fonts",
    "copy:images": "copy './node_modules/uswds/dist/img/**/*.*' ./src/img",
    "copy:assets": "npm run copy:fonts && npm run copy:images",
    "postinstall": "npm run clean && npm run copy:assets",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GSA/sam-styles.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@frctl/fractal": "^1.1.7",
    "@frctl/mandelbrot": "^1.2.0",
    "@frctl/nunjucks": "^1.0.3",
    "uswds": "2.0.0-beta.6"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "axe-core": "^3.2.2",
    "chrome-launcher": "^0.10.5",
    "chrome-remote-interface": "^0.27.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy": "^0.3.2",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "fractal-webpack-plugin": "^3.0.0",
    "jest": "^24.5.0",
    "jest-environment-jsdom": "^24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "yargs": "^13.2.2"
  }
}
