  "main": "index.js",
  "scripts": {
    "fractal:build": "fractal build",
    "federalist": "npm install --only=dev && npm run prod && npm run fractal:build",
    "start": "fractal start --sync",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "prod": "npm run webpack -- --env.mode production",
    "clean": "rimraf dist",
    "copy:fonts": "cp -r node_modules/uswds/dist/fonts/* src/fonts/",
    "copy:images": "cp -r node_modules/uswds/dist/img/* src/img/",
    "copy:assets": "npm run copy:fonts && npm run copy:images",
    "postinstall": "npm run clean && npm run copy:assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GSA/sam-styles.git"
  },
