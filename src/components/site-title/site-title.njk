<div class="usa-navbar">
  <div class="usa-logo" id="{{ header.primary.id_prefix }}logo">
    <em class="usa-logo__text"><a href="{{ header.link.href | default('/') }}" title="{{ header.link.title | default('Home') }}" aria-label="{{ header.link.label | default('Home') }}">{{ header.title | default(site.title) }}</a></em>
  </div>
  <button class="usa-menu-btn">
    <span class="icon-layers icon-fw icon-3x">
      <i class="fas icon-square"></i>
      <i class="sam icon-bars" data-fa-transform="shrink-6"></i>
      <span class="icon-layers-counter"></span>
    </span>
    <span class="usa-sr-only">Close</span>
  </button>
</div>
